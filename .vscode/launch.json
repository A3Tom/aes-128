{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node/src/main.ts",
      "preLaunchTask": "tsc: build - node/tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/node/dist/**/*.js"
      ]
    }
  ]
}